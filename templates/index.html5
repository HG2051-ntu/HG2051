<!DOCTYPE html>
<html xmlns="http://www.w3.org/1999/xhtml" lang="$lang$" xml:lang="$lang$"$if(dir)$ dir="$dir$"$endif$>
<head>
  <meta charset="utf-8" />
  <meta name="generator" content="pandoc" />
  <meta name="viewport" content="width=device-width, initial-scale=1.0, user-scalable=yes" />
$for(author-meta)$
  <meta name="author" content="$author-meta$" />
$endfor$
$if(date-meta)$
  <meta name="dcterms.date" content="$date-meta$" />
$endif$
$if(keywords)$
  <meta name="keywords" content="$for(keywords)$$keywords$$sep$, $endfor$" />
$endif$
  <title>$if(title-prefix)$$title-prefix$ – $endif$$pagetitle$</title>
  <style type="text/css">
      code{white-space: pre-wrap;}
      span.smallcaps{font-variant: small-caps;}
      span.underline{text-decoration: underline;}
      div.column{display: inline-block; vertical-align: top; width: 50%;}
$if(quotes)$
      q { quotes: "“" "”" "‘" "’"; }
$endif$
  </style>
$if(highlighting-css)$
  <style type="text/css">
$highlighting-css$
  </style>
$endif$
  <link href="https://fonts.googleapis.com/css2?family=Fira+Sans&display=swap" rel="stylesheet">
$for(css)$
  <link rel="stylesheet" href="$css$" />
$endfor$
$if(math)$
  $math$
$endif$
$for(header-includes)$
  $header-includes$
$endfor$
</head>
<body>
$for(include-before)$
$include-before$
$endfor$
$if(title)$
<header id="title-block-header">
<h1 class="title">$if(title-prefix)$$title-prefix$ – $endif$$title$</h1>
$if(subtitle)$
<p class="subtitle">$subtitle$</p>
$endif$
$-- Instructor Listing ---------
$for(instructor)$
<p class="instructor">$instructor.name$$if(instructor.email)$ &lt;<a href="mailto:$instructor.email$$if(title-prefix)$?subject=[$title-prefix$]$endif$">$instructor.email$</a>&gt;$endif$</p>
$endfor$
$if(date)$
<p class="date">$date$</p>
$endif$
$if(location)$
<p class="location">$location$</p>
$endif$
</header>
$endif$
<main>
$-- Course Description ---------
$if(abstract)$
<p class="abstract">
$abstract$
</p>
$endif$
$-- Schedule Table -------------
$if(schedule)$
<h2>Schedule</h2>
<table class="schedule">
  <thead>
    <tr>
      <td>Week</td>
      <td>Date</td>
      <td>Topic</td>
    </tr>
  </thead>
$for(schedule)$
  <tbody>
    <tr $if(schedule.cancelled)$class="cancelled"$endif$>
      <td>$schedule.week$</td>
      <td>$schedule.date$</td>
      <td>$if(schedule.url)$<a href="$schedule.url$">$schedule.topic$</a>$else$$schedule.topic$$endif$</td>
    </tr>
  </tbody>
$endfor$
</table>
$endif$
</main>
$-- Main Body ------------------
$body$
$-- Footers --------------------
$for(include-after)$
$include-after$
$endfor$
</body>
</html>
